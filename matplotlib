# matplotlib and numpy must be imported
import matplotlib.pyplot as plt
import numpy as np

# for every minute, hour or day (x) move high, low, close, open, volumn (y's) into own list
# let x always be your date/time and y's be your close/open/high/low/weight/volumn

highs = [9, 2, 3, 6, 21, 5]
lows = [1, 2, 3, 7, 5, 3]

x = [1, 2, 3, 4, 5, 6]
y = highs
z = lows

plt.plot(x, y)
plt.plot(x, z)
plt.show()
